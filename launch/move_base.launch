<launch>
  <arg name="cmd_vel_topic" default="cmd_vel"/>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find multirobot_stage)/scripts/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find multirobot_stage)/scripts/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find multirobot_stage)/scripts/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find multirobot_stage)/scripts/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find multirobot_stage)/scripts/base_local_planner_params.yaml" command="load" />
    <remap from="map" to="raw_merged_map"/>
    <remap from="cmd_vel" to="$(arg cmd_vel_topic)"/>
  </node>
</launch>
